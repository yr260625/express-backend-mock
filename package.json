{
  "name": "express-backend-mock",
  "version": "1.0.0",
  "description": "",
  "main": "build/app.js",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn src/app.ts",
    "start": "node dist/app.js",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "typeorm": "env-cmd -f .env typeorm-ts-node-commonjs -d src/data-source.ts",
    "migration:gen": "npm run typeorm migration:generate",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:show": "npm run typeorm migration:show",
    "local-start": "env-cmd -f .env.local npm run dev",
    "local-run": "npm run build && env-cmd -f .env npm start",
    "local-typeorm": "env-cmd -f .env.local typeorm-ts-node-commonjs -d src/data-source.ts",
    "local-migration:gen": "npm run local-typeorm migration:generate",
    "local-migration:run": "npm run local-typeorm migration:run",
    "local-migration:revert": "npm run local-typeorm migration:revert",
    "local-migration:show": "npm run local-typeorm migration:show"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "moment": "^2.29.4",
    "mongodb": "^5.8.1",
    "mongoose": "^7.4.5",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.5.7",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "prettier": "^3.0.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
